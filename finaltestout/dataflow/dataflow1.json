{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "campaignanalyticscsv",
						"type": "DatasetReference"
					},
					"name": "campaignanalytics"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "MapCampaignAnalytics"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_Col0_} as string,",
				"          {_Col1_} as string,",
				"          {_Col2_} as string,",
				"          {_Col3_} as string,",
				"          {_Col4_} as string,",
				"          {_Col5_} as double,",
				"          {_Col6_} as string,",
				"          {_Col7_} as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> campaignanalytics",
				"campaignanalytics select(mapColumn(",
				"          Region = {_Col0_},",
				"          Country = {_Col1_},",
				"          ProductCategory = {_Col2_},",
				"          CampaignName = {_Col3_},",
				"          RevenuePart1 = {_Col4_},",
				"          Revenue = {_Col5_},",
				"          RevenueTargetPart1 = {_Col6_},",
				"          RevenueTarget = {_Col7_}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> MapCampaignAnalytics"
			]
		}
	}
}